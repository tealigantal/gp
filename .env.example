# Example environment file for docker-compose (.env)
# Copy to .env and edit values; NEVER commit real secrets

# Timezone
TZ=Asia/Shanghai

# LLM (DeepSeek). Leave empty to let /chat degrade gracefully
LLM_BASE_URL=https://api.deepseek.com/v1
LLM_API_KEY=
CHAT_MODEL=deepseek-chat

# Data provider (akshare | local | official)
DATA_PROVIDER=akshare

# Optional: request timeout for outbound HTTP in seconds
GP_REQUEST_TIMEOUT_SEC=60

